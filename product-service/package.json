{
  "name": "product-service",
  "version": "1.0.0",
  "main": "handler.js",
  "scripts": {
    "test": "npm run swagger && NODE_ENV=test jest --ci --verbose",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "buildtest": "tsc --noEmit",
    "swagger": "tsoa spec",
    "local:invoke:get-products": "sls invoke local --function get-products-controller",
    "local:invoke:get-by-id": "sls invoke local --function get-products-by-id-controller",
    "local:invoke:get-swagger": "npm run swagger && sls invoke local --function get-swagger-file-controller",
    "local:start": "npm run swagger && sls offline start --skipCacheInvalidation",
    "package": "npm run swagger && sls package",
    "deploy": "npm run swagger && sls deploy",
    "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.js",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:generate": "yarn typeorm migration:generate -n"
  },
  "dependencies": {
    "@dazn/lambda-powertools-cloudwatchevents-client": "^1.26.0",
    "@dazn/lambda-powertools-correlation-ids": "^1.24.1",
    "@dazn/lambda-powertools-dynamodb-client": "^1.26.0",
    "@dazn/lambda-powertools-kinesis-client": "^1.26.0",
    "@dazn/lambda-powertools-lambda-client": "^1.26.0",
    "@dazn/lambda-powertools-logger": "^1.24.1",
    "@dazn/lambda-powertools-pattern-basic": "^1.27.0",
    "@dazn/lambda-powertools-sns-client": "^1.26.0",
    "@dazn/lambda-powertools-sqs-client": "^1.26.0",
    "@middy/core": "^1.4.0",
    "@middy/http-cors": "^1.4.0",
    "@middy/http-json-body-parser": "^1.4.0",
    "@middy/validator": "^1.4.0",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.773.0",
    "http-errors": "^1.8.0",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "serverless": "^2.8.0",
    "source-map-support": "^0.5.19",
    "tsoa": "^3.4.0",
    "typeorm": "^0.2.29",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.64",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-module-resolver": "^0.16.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.6.1",
    "serverless-offline": "^5.12.1",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.4.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.9.7",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.44.2",
    "webpack-node-externals": "^1.7.2"
  }
}
